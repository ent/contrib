    "/categories/{id}": {
      "get": {
        "tags": [
          "Category"
        ],
        "summary": "Find a Category by ID",
        "description": "Finds the Category with the requested ID and returns it.",
        "operationId": "readCategory",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the Category",
            "schema": {
              "type": "integer",
              "format": "int32"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Category with requested ID was found"
          }
        }
      }
    },
