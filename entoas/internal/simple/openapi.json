{
  "openapi": "3.0.3",
  "info": {
    "title": "My Simple API",
    "description": "API to demonstrate **simple model** generation.",
    "version": "0.0.1"
  },
  "paths": {
    "/categories/{id}": {
      "get": {
        "tags": [
          "Category"
        ],
        "description": "Finds the Category with the requested ID and returns it.",
        "operationId": "readCategory",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the Category",
            "schema": {
              "type": "integer",
              "format": "int32"
            },
            "required": true
          }
        ]
      }
    },
    "/pets/{id}": {
      "get": {
        "tags": [
          "Pet"
        ],
        "description": "Finds the Pet with the requested ID and returns it.",
        "operationId": "readPet",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the Pet",
            "schema": {
              "type": "integer",
              "format": "int32"
            },
            "required": true
          }
        ]
      }
    },
    "/users/{id}": {
      "get": {
        "tags": [
          "User"
        ],
        "description": "Finds the User with the requested ID and returns it.",
        "operationId": "readUser",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the User",
            "schema": {
              "type": "integer",
              "format": "int32"
            },
            "required": true
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "Category": {
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string"
          },
          "pets": {
            "$ref": "#/components/schemas/Pet"
          }
        },
        "required": [
          "id",
          "name"
        ]
      },
      "Pet": {
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string"
          },
          "nicknames": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "age": {
            "type": "integer",
            "format": "int32"
          },
          "categories": {
            "$ref": "#/components/schemas/Category"
          },
          "owner": {
            "$ref": "#/components/schemas/User"
          },
          "friends": {
            "$ref": "#/components/schemas/Pet"
          }
        },
        "required": [
          "id",
          "name"
        ]
      },
      "User": {
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string"
          },
          "age": {
            "type": "integer",
            "format": "int32"
          },
          "pets": {
            "$ref": "#/components/schemas/Pet"
          }
        },
        "required": [
          "id",
          "name",
          "age"
        ]
      }
    }
  }
}